{
  "openapi": "3.0.0",
  "info": {
    "title": "api-scheduling",
    "description": "Scheduling Module - API Structure",
    "contact": {
      "name": "Bookiteasy",
      "url": "www.bookiteasy.com",
      "email": "noreply@bookiteasy.com"
    },
    "version": "1.0.0"
  },
  "paths": {
    "/agenda": {
      "get": {
        "summary": "List Agendas",
        "description": "Retrieve All",
        "parameters": [
          {
            "name": "${client_account}",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {}
            }
          }
        },
        "requestBody": {}
      },
      "post": {
        "summary": "Create Agenda",
        "description": "Create One",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AgendaContainer"
              }
            }
          },
          "required": true
        }
      }
    },
    "/agenda/{agendaId}": {
      "get": {
        "summary": "Retrieve Agenda",
        "responses": {
          "200": {}
        }
      },
      "put": {
        "summary": "Update Agenda",
        "responses": {
          "200": {}
        },
        "requestBody": {
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "string"
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create an AgendaItem in current Agenda",
        "responses": {
          "201": {},
          "400": {},
          "401": {}
        },
        "requestBody": {}
      },
      "delete": {
        "summary": "Delete an Agenda",
        "responses": {
          "204": {},
          "400": {},
          "401": {}
        }
      },
      "parameters": [
        {
          "name": "agendaId",
          "in": "path",
          "description": "The agendaId",
          "required": true,
          "schema": {
            "type": "string"
          }
        },
        {
          "name": "${client_account}",
          "in": "query",
          "schema": {
            "type": "string"
          }
        }
      ]
    },
    "/agenda/{agendaId}/{agendaItemId}": {
      "get": {
        "summary": "get Item"
      },
      "put": {
        "summary": "Create Item"
      }
    }
  },
  "components": {
    "schemas": {
      "AgendaContainer": {
        "title": "Root Type for AgendaContainer",
        "description": "The root of the AgendaContainer type's schema.",
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "userId": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "dateAdded": {
            "format": "int64",
            "type": "integer"
          },
          "dateUpdated": {
            "format": "int64",
            "type": "integer"
          },
          "locationId": {
            "type": "string"
          }
        }
      },
      "AgendaList": {
        "properties": {
          "userId": {
            "type": "string"
          },
          "agendaContainerId": {
            "type": "string"
          },
          "agendaListDisplayOrderId": {
            "type": "integer"
          }
        }
      },
      "User": {
        "title": "Root Type for User",
        "description": "The root of the User type's schema.",
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "displayName": {
            "type": "string"
          },
          "publicProfileName": {
            "type": "string"
          },
          "publicProfilePicture": {
            "type": "string"
          }
        }
      },
      "AgendaItem": {
        "title": "Root Type for AgendaItem",
        "description": "The root of the AgendaItem type's schema.",
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "remoteId": {
            "type": "string"
          },
          "agendaId": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "withUser": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/User"
            }
          },
          "startTime": {
            "format": "int32",
            "type": "integer"
          },
          "endTime": {
            "format": "int32",
            "type": "integer"
          },
          "transactionId": {
            "type": "string"
          },
          "owner": {
            "type": "boolean"
          },
          "approval": {
            "$ref": "#/components/schemas/AgendaItemApproval"
          }
        }
      },
      "AgendaItemApproval": {
        "title": "Root Type for AgendaItemApproval",
        "description": "The root of the AgendaItemApproval type's schema.",
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "self": {
            "type": "boolean"
          },
          "approvedBy": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/User"
            }
          },
          "statusTotal": {
            "type": "boolean"
          }
        }
      }
    },
    "securitySchemes": {
      "Bookiteasy OAuth2": {
        "type": "oauth2",
        "description": "Security System OAuth2",
        "flows": {
          "clientCredentials": {
            "tokenUrl": "https://auth.bookiteasy.com/auth/realms/public/protocol/openid-connect/token",
            "refreshUrl": "https://auth.bookiteasy.com/auth/realms/public/protocol/openid-connect/token",
            "scopes": {}
          }
        }
      }
    }
  }
}
